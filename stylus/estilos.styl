@font-face 
	font-family: 'font_menu'
	src: url('../fonts/Helvetica.eot')
	src: local('â˜º'), url('../fonts/Helvetica.woff') format('woff'), url('../fonts/Helvetica.ttf') format('truetype'), url('../fonts/Helvetica.svg') format('svg')
	font-weight: normal
	font-style: normal


tam_font_cont = .9em
btn_morado = #3e092d

a
	text-decoration none

li
	list-style none

img
textarea
	max-width 100%

body, html
	background-color #020106
	font-family 'Roboto Slab', sans-serif
	height 100%
	color #ffffff
	height 100%
	width 100%
	-webkit-overflow-scrolling touch

.load
	background #000000 url(../imgs/load_mondo.gif) no-repeat center center
	background-size 10%
	position fixed
	z-index 999
	width 100%
	height 100%

.menu_desktop
.l_accion_desktop
	display none
	
.fondo_gral
	background url(../imgs/fondo_seccion_all.jpg) bottom center no-repeat
	background-size 100% auto
	width 100%
	-webkit-overflow-scrolling touch
	

.section
	min-height 570px

		
.menu_movil
	display none
	background url(../imgs/fondo_menu_movil.jpg) repeat center
	position fixed
	height 100%
	width 100%
	z-index 10
	overflow-y auto
	.menu_circulo
		overflow-y auto		
		position absolute
		background url(../imgs/menu_circulo_azul.png) no-repeat left center
		background-size 100% 100%
		font-size .8em
		height 100%
		margin-left -100%
		width 100%
		.menu_conten
			display none
			margin 0 auto
			width 90%
			ul
				padding 0
			.sec_logo
				display flex
				display -webkit-flex
				display -ms-flex
				display -moz-flex
				-webkit-justify-content space-between
				justify-content space-between
				padding-top 1em
				width 100%
				.menu_logo
					height 48px
					padding 0 1.5em .7em .5em
				.menu_btn_close
					height 25px
					cursor pointer
					&:hover
						-webkit-transform rotate(180deg)
						-moz-transform rotate(180deg)
						-o-transform rotate(180deg)
						writing-mode lr-tb
			.nav_options
				border-top .2em dashed #ffffff
				width 55%
				max-width 260px
				a 
					color #ffffff
					display block
					margin .5em 0
					padding .5em 1em
			.nav_links
				border-top .2em dashed #ffffff
				padding 0 1em
				width 65%
				max-width 260px
				li img, li a
					display inline-block
					vertical-align middle
					padding .2em 0
					color #ffffff
					img
						height 30px
			.nav_rds
				border-top .2em dashed #ffffff
				width 50%
				padding-top 1em
				max-width 260px
				img
					height 35px
					margin 0 .5em

.fxd_cambio
	background rgba(35,166,223,.6)
	
.menu_movil_fxd
	position fixed
	top 0
	width 100%
	z-index 5
	div
		margin 0 auto
		width 88%
		.fxd_logo
			display none
			height 48px
			padding .4em 0
		.fxd_btn_menu
			cursor pointer
			float right
			top 20px
			height 20px
			position relative


// --- Clases Comunes ---
.centrado
	// background olive
	margin 0 auto
	text-align center
	width 90%
	
.cambio
	-webkit-transition all .4s ease-out
	transition all .4s ease-out

// --- Secc. Inicio Movil ---
.inicio
	display flex
	display -moz-flex
	display -ms-flex
	display -webkit-flex
	align-items center
	-webkit-align-items center
	webkit-justify-content center
	justify-content center
	.inicio_cont >	img 
		height 250px
	.inicio_cont div
		display flex
		display -moz-flex
		display -ms-flex
		display -webkit-flex		
		align-items center
		-webkit-align-items center
		flex-direction row
		-webkit-flex-direction row
		webkit-justify-content center
		justify-content center
		h3
			font-weight 400
			font-size tam_font_cont
			padding 0 .5em
			-moz-box-sizing border-box
			-ms-box-sizing border-box
			-webkit-box-sizing border-box
			box-sizing border-box
		.line_lf
			background url(../imgs/line_left.png) repeat-x right center
			background-size container
			height 5px
			width 12%
		.line_rg
			background url(../imgs/line_right.png) repeat-x left center
			background-size container
			height 5px
			width 12%
	.rds_home a 
		margin-top 15%
		img
			height 35px
			padding 0 .75em
			width 35px
	
	.lgo_desktop
		display none


// --- Secc. Quienes-Somos ---
.q_somos
	font-family 'font_menu'
	font-size tam_font_cont
	font-weight 300
	display flex
	display -moz-flex
	display -ms-flex
	display -webkit-flex
	align-items center
	-webkit-align-items center
	webkit-justify-content center
	justify-content center
	.q_somos_cont
		border .2em dashed #ffffff
		border-radius .5em
		text-align left
	p
		margin-top 20%
		margin-bottom 0
		padding 0 1em
	.imgs_right
		float right
		position relative
		right -14px
		top -17px
		height 70px
	.imgs_left
		bottom -40px
		left -8px
		position relative
		height 80px
		

// --- Secc. Linea de Accion Movil---
.l_accion_movil
	padding 14% 0
	width 100%
	.la_rueda
		background url(../imgs/fondo_linea.png) no-repeat center 
		background-size 100% 
		padding 3em
		margin-left -3em
	#la1_movil
		z-index 1
	#la2_movil
		z-index 2
		left -50px	
	#la3_movil
		z-index 3
		left -100px	
	#la4_movil
		z-index 4
		left -150px	
	#la5_movil
		z-index 5
		left -200px
	

.l_accion_cont
	margin 0 auto
	margin-top 13%
	margin-bottom 35%
	text-align center
	width 85%
	.la_img
		height 130px
	h2
		padding 0
		width 85%
		margin 0 auto
		font-weight 400
		text-align left
	.info_accion
		background rgba(0,0,0,.3)
		border .2em dashed #ffffff
		border-radius .5em
		font-weight 400
		font-size .8em
		margin 0 auto
		padding 1em
		height 110px
		text-align left
		width 80%
		position relative
		z-index 100
	.ico_estrella
		float right
		position relative
		top -21px
		height 50px


// --- Secc. Colaboracion Movil ---
.colabo
	font-size .8em
	display flex
	display -moz-flex
	display -ms-flex
	display -webkit-flex
	align-items center
	-webkit-align-items center
	webkit-justify-content center
	justify-content center
	.colabo_cont
		.colabo_grupo
			text-align center
			img
				width 95%
		.colabo_info 
			div:first-child
				margin-top .5em
				padding-top .5em
				border-top .2em dashed #ffffff
				width 95%
			div
				display flex
				display -webkit-flex
				display -ms-flex
				display -moz-flex
				-webkit-justify-content space-between
				justify-content space-between
				align-items center
				-webkit-align-items center
				margin 0 auto
				text-align left
				width 95%
				img
					height 50px
					margin-right 1em
				p
					width 100%


// --- Secc. Contacto MOVIL ---
.contacto
	font-size tam_font_cont
	display flex
	display -moz-flex
	display -ms-flex
	display -webkit-flex
	align-items center
	-webkit-align-items center
	webkit-justify-content center
	justify-content center
	width 100%
	.contacto_cont
		text-align left
		word-wrap break-word
		form 
			width 100%
			div
				input[type="text"]
				input[type="email"]
				textarea
					width 95%
					background transparent
					border 0px
					border-bottom .1em solid #ffffff !important
					padding .1em .5em
				textarea
					border .1em solid #ffffff !important
					margin-top 1em
			.submit
				text-align right
				width 97%
				input[type="submit"]
					background #ffffff
					border 0px
					color #333333
					padding .4em 0
					width 27%
					&:hover
						background btn_morado
						color #ffffff
						-webkit-box-shadow 5px 5px 5px 0px rgba(0,0,0,0.75)
						-moz-box-shadow 5px 5px 5px 0px rgba(0,0,0,0.75)
						box-shadow 5px 5px 5px 0px rgba(0,0,0,0.75)
		.contac_info
			font-size .9em
			width 100%
			address
				margin 1em 0
				font-style normal
						

#nombre-error,
#correo-error,
#asunto-error,
#mensajes-error
	text-shadow 5px 2px 5px #0d0d0d	
	font-style italic	
	display block
	text-align right


// --- Secc. Newsletter MOVIL ---
.new_ltr
	background rgba(0,96,100,.5)	
	-webkit-box-shadow 0px 6px 5px 0px rgba(0,0,0,0.75)
	-moz-box-shadow 0px 6px 5px 0px rgba(0,0,0,0.75)
	box-shadow 0px 6px 5px 0px rgba(0,0,0,0.75)
	.new_ltr_cont
		h2
			font-size 1.3em
			margin 0
			padding .5em 0
		div
			input[type="text"]
				background #ffffff
				border 0px
				color #333333
				font-size tam_font_cont
				width 90%
				padding .5em
				-webkit-box-shadow 5px 5px 5px 0px rgba(0,0,0,0.75)
				-moz-box-shadow 5px 5px 5px 0px rgba(0,0,0,0.75)
				box-shadow 5px 5px 5px 0px rgba(0,0,0,0.75)
			.new_lt_info
				padding 1em 0
				div
				input[type="submit"]
					display inline-block
					font-size .8em
					vertical-align top
				div
					text-align left
					width 60%
				input[type="submit"]
					width 35%
					background btn_morado
					font-size 1em
					border 0px
					color #ffffff
					padding .5em .25em
					width 36%
					-webkit-box-shadow 5px 5px 5px 0px rgba(0,0,0,0.75)
					-moz-box-shadow 5px 5px 5px 0px rgba(0,0,0,0.75)
					box-shadow 5px 5px 5px 0px rgba(0,0,0,0.75)
					&:hover
						background #ffffff
						color #333333
					

				
// --- Footer MOVIL ---
footer
	background btn_morado
	color #ffffff	
	text-align center
	width 100%
	font-size .8em
	font-family 'Roboto', sans-serif;
	letter-spacing 1px
	padding 1em 0
	.ftr_links
		a
			color #ffffff
			display block
			margin 1.5em 0
	.ftr_rds
		div
			img
				height 40px
				margin 1em
		p
			border-top .2em solid #ffffff
			letter-spacing 1px
			padding 2em 0
			margin 0 auto
			width 90%
			
	

/* agenda */
.sec_agenda_cont
	padding-top 30%
	
.agenda-container
	height 85%
	font-size .8em
	margin 0 auto
	width 95%

.agenda-viewport::-webkit-scrollbar, .fundacion-container::-webkit-scrollbar, .colaboracion-container::-webkit-scrollbar
	width: 4px	
	background transparent

.agenda-viewport::-webkit-scrollbar-thumb, .fundacion-container::-webkit-scrollbar-thumb, .fundacion-container::-webkit-scrollbar-thumb::-webkit-scrollbar-thumb
	background rgba(255,255,255,0.2)



	
.agenda-viewport
	height 80%	
	width 50%
	position absolute
	overflow-y auto
	// position relative

.agenda-pager
	text-align right
	height 85%
	position absolute
	border-right 2px solid white	
	right 10px
	width 49%
	padding-top 5px

.month
	margin-bottom 70px

.month-name
	font-size 1.100em
	text-transform uppercase
	
.agenda-pager,
.header2
	font-size 1.3em
	// padding 0
	margin 0
	font-weight 700

.events
	border-top 1px solid rgba(255,255,255,0.7)
	border-bottom 1px solid rgba(255,255,255,0.7)
	display inline-block
	padding-top 25px
	width 100%

	.row				
		width 100%
		display inline-block

.event
	float left
	padding 0 3% 50px 3%
	padding-bottom 5%
	width 100%

	.event-header
		padding-left 5px
		width 100%
		min-height 30px

	.event-title
		text-transform uppercase		
		
	
	.event-date
		font-size .9em		
		margin-top 1em
		text-transform uppercase

	.event-body
		padding-left 5px

	p
		letter-spacing 0.1em
		font-size 1.00em
		margin 20px 0		
		color rgba(255,255,255,0.8)

.addeventatc        
	// font-family HelveticaNeueLTStd-Th
	font-weight 300
	color #ffffff
	text-decoration none
	font-size 0.620em   
	border 1px #ffffff solid
	letter-spacing 0.150em
	padding 5px 12px
	text-transform uppercase

	&:hover
		color #ffffff
		font-size 0.620em   
		text-decoration none

.addeventatc_dropdown
	span
		font-size 1.100em

		&:hover
			font-size 1.100em

	em
		font-size 0.800em!important

.pager-header
	width 100%
	font-size 0.750em
	border-right 5px solid #ffffff	
	padding 8px 10px 8px 0

.pager, .pager-event
	list-style none
	margin 0
	padding 0

.pager
	margin-top 20px
	padding 0
	color rgba(255,255,255,0.8)
	text-transform uppercase

	.month-maker
		font-size 0.50em
		letter-spacing 0.1em
		cursor pointer

		&:hover
			color white

		.marker
			background rgba(255,255,255,0.8)
			display inline-block
			width 30px
			height 5px
			transition all 500ms ease

		&:hover .marker		
			background #ffffff
			width 35px
			transition all 500ms ease
		
.event-marker						
	cursor pointer	
	padding 5px 0
	display inline-block
	width 100%

	.marker
		background #cecece				
		display block
		float right
		height 3px				
		width 20px
		transition all 500ms ease
		margin 0
		padding 0

	&:hover .marker
		background white
		height 8px
		width 40px
		transition all 500ms ease		

